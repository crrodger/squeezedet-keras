    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Score.py",
            "type": "python",
            "request": "launch",
            "program": "score.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceRoot}",
            },
            "args": [
                "--checkpoint=model.28-3.75.hdf5",
            ],
            "cwd": "${fileDirname}",
        }
    ]
}